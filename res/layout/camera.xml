<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/tab_cam_black" >

    <TableLayout
        android1:id="@+id/tableLayout1"
        android1:layout_width="wrap_content"
        android1:layout_height="fill_parent"
        android1:layout_alignParentRight="true"
        android1:layout_alignParentTop="true"
        android:background="@drawable/tab_cam_background" >

        <TableRow
            android:layout_marginLeft="5dp"
            android:layout_weight="0.6"
            android:gravity="center" >

            <ImageView
                android1:id="@+id/button1"
                android1:layout_width="wrap_content"
                android1:layout_height="wrap_content"
                android1:src="@drawable/ep_logo" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:gravity="center" >

            <ImageView
                android1:id="@+id/button2"
                android1:layout_width="wrap_content"
                android1:layout_height="wrap_content"
                android1:src="@drawable/tab_cam_shot" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:gravity="center" >

            <ImageView
                android1:id="@+id/button3"
                android1:layout_width="wrap_content"
                android1:layout_height="wrap_content"
                android1:src="@drawable/tab_cam_rec" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:gravity="center" >

            <ImageView
                android1:id="@+id/button4"
                android1:layout_width="wrap_content"
                android1:layout_height="wrap_content"
                android1:src="@drawable/tab_cam_listen" />
        </TableRow>
        
        <TableRow
            android:layout_weight="0.5"
            android:gravity="center" >

            <ImageView
                android1:id="@+id/button6"
                android1:layout_width="wrap_content"
                android1:layout_height="wrap_content"
                android1:src="@drawable/tab_cam_night" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:gravity="center" >

            <ImageView
                android1:id="@+id/button5"
                android1:layout_width="wrap_content"
                android1:layout_height="wrap_content"
                android1:src="@drawable/tab_cam_back" />
        </TableRow>

        
    </TableLayout>

    <LinearLayout
        android1:id="@+id/linearLayout1"
        android1:layout_width="wrap_content"
        android1:layout_height="fill_parent"
        android1:layout_alignParentTop="true"
        android1:layout_marginRight="15dp"
        android1:layout_toLeftOf="@+id/tableLayout1"
        android1:gravity="center"
        android1:orientation="vertical" >

        <ImageView
            android1:id="@+id/button7"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:src="@drawable/tab_cam_add" />

        <ImageView
            android1:id="@+id/button8"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:layout_marginTop="20dp"
            android1:src="@drawable/tab_cam_del" />

        <ImageView
            android1:id="@+id/button11"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:layout_marginTop="70dp"
            android1:src="@drawable/tab_cam_up" />

        <LinearLayout
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:orientation="horizontal" >

            <ImageView
                android1:id="@+id/button9"
                android1:layout_width="wrap_content"
                android1:layout_height="wrap_content"
                android1:src="@drawable/tab_cam_left" />

            <ImageView
                android1:id="@+id/button10"
                android1:layout_width="wrap_content"
                android1:layout_height="wrap_content"
                android1:layout_marginLeft="20dp"
                android1:src="@drawable/tab_cam_right" />
        </LinearLayout>

        <ImageView
            android1:id="@+id/button12"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:src="@drawable/tab_cam_down" />
    </LinearLayout>

</RelativeLayout>